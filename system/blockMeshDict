FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 0.01;
h1       0.75;
h2       0.75;
H         2.5;
x0        -7.5;
x1        -7.0;
x3        -3.5;
x4        3.5;
vertices
(
    ($x0 0 0) //    0
    ($x1 0 0) //    1
    ($x3 0 0) //   2
    ($x4 0 0) //    3

    ($x0 $h1 0) //    4
    ($x1 $h1 0) //    5
    ($x3 $h2 0) //    6
    ($x4 $h2 0) //    7

    ($x0 $H 0) //     8
    ($x1 $H 0) //    9
    ($x3 $H 0) //   10
    ($x4 $H 0) //    11

    ($x0 $h1 1) //  12
    ($x1 $h1 1) //    13
    ($x3 $h2 1) //    14

    ($x4 $h2 1) //    15
    ($x0 $H 1) //  16
    ($x1 $H 1) //    17
    ($x3 $H 1) //    18
    ($x4 $H 1) //    19

    ($x0 0 1) //    20
    ($x1 0 1) //    21
    ($x3 0 1) //   22
    ($x4 0 1) //    23

);

blocks
(
    hex (0 1 5 4 20 21 13 12) (10 10 1) simpleGrading (1 1 1)
    hex (2 3 7 6 22 23 15 14) (15 15 1) simpleGrading (1 1 1)
    hex (4 5 9 8 12 13 17 16) (10 10 1) simpleGrading (1 1 1)
    hex (5 6 10 9 13 14 18 17) (25 10 1) simpleGrading (1 1 1)
    hex (6 7 11 10 14 15 19 18) (15 10 1) simpleGrading (1 1 1)
);

