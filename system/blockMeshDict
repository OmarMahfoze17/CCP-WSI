FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;
y0        -0.7;
h1        -0.;
H         0.45;
x0        -4;
x1        0;
x2        4;
vertices
(
    ($x0 $y0 0) //    0
    ($x1 $y0 0) //    1
    ($x1 $h1 0) //   2
    ($x0 $h1 0) //    3
    ($x2 $y0 0) //    4
    ($x2 $h1 0) //    5
    ($x1 $H 0) //    6
    ($x0 $H 0) //    7

    ($x0 $y0 1) //    8
    ($x1 $y0 1) //    9
    ($x1 $h1 1) //   10
    ($x0 $h1 1) //    11
    ($x2 $y0 1) //    12
    ($x2 $h1 1) //    13
    ($x1 $H 1) //    14
    ($x0 $H 1) //    15
);

blocks
(
    hex (0 1 2 3 8 9 10 11) (40 40 1) simpleGrading (1 1 1)
    hex (1 4 5 2 9 12 13 10) (40 40 1) simpleGrading (3 1 1)
    hex (3 2 6 7 11 10 14 15) (40 20 1) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
    movingWall
    {
        type wall;
        faces
        (
            (2 6 14 10)
        );
    }

    movingTopWall
    {
        type wall;
        faces
        (
            (2 10 13 5)
        );
    }

    atmosphere
    {
        type patch;
        faces
        (
            (6 7 15 14)
        );
    }
    outlet
    {
        type wall;
        faces
        (
            (13 12 4 5)
        );
    }
    axis
    {
        type patch;
        faces
        (
            (0 1 9 8)  //???????????????????????
            (1 4 12 9)
        );
    }
    inlet
    {
        type patch;
        faces
        (
            (0 8 11 3)
            (3 11 15 7)
        );
    }

    back
    {
        type empty;
        faces
        (
            (0 3 2 1)
            (3 7 6 2)
            (1 2 5 4)
        );
    }
    front
    {
        type empty;
        faces
        (
            (8 9 10 11)
            (11 10 14 15)
            (9 12 13 10)

        );
    }
